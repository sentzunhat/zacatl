{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../build/scripts",
    "rootDir": ".",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "target": "ES2022",
    "esModuleInterop": true,
    "declaration": false,
    "declarationMap": false,
    "skipLibCheck": true
  },
  "include": ["*.ts"]
}
