  "repository": {
    "type": "git",
    "url": "https://github.com/sentzunhat/zacatl.git"
  },
  "description": "A modular, high-performance TypeScript microservice framework for Node.js, featuring layered architecture, dependency injection, and robust validation for building scalable APIs and distributed systems.",
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=10.0.0"
  },
  "volta": {
    "node": "24.0.0"
  },
  "keywords": [
    "typescript",
    "microservice",
    "microservices",
    "framework",
    "nodejs",
    "dependency-injection",
    "hexagonal-architecture",
    "layered-architecture",
    "clean-architecture",
    "fastify",
    "mongodb",
    "mongoose",
    "zod",
    "validation",
    "vitest",
    "testing",
    "api",
    "rest-api",
    "backend",
    "server",
    "enterprise",
    "scalable",
    "modular",
    "tsyringe",
    "pino",
    "logging",
    "i18n",
    "internationalization",
    "artisan"
  ],
  "author": "Zacatl Contributors",
  "bugs": {
    "url": "https://github.com/sentzunhat/zacatl/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "npx tsx src/index.ts --project ./tsconfig.json",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "build:watch": "tsc --project tsconfig.json --watch",
    "test": "NODE_ENV=test ENV=test vitest run",
    "test:watch": "NODE_ENV=test ENV=test vitest",
    "test:coverage": "npm run test -- --coverage",
    "type:check": "npm run type:check:src && npm run type:check:test",
    "type:check:src": "tsc --noEmit -p ./tsconfig.json",
    "type:check:test": "tsc --noEmit -p ./test/tsconfig.json",
    "lint": "eslint src",
    "clean:modules": "rm -rf node_modules && npm run clean:modules:examples",
    "clean:modules:main": "rm -rf node_modules",
    "clean:modules:examples": "rm -rf examples/*/node_modules examples/*/*/node_modules examples/*/*/*/node_modules examples/*/*/*/*/node_modules 2>/dev/null; true",
    "clean:modules:all": "npm run clean:modules",
    "clean:build": "rm -rf build dist *.tsbuildinfo",
    "clean": "npm run clean:build && npm run clean:modules:all",
    "prepublish": "npm run test:coverage && npm run type:check && npm run lint && npm run build",
    "publish:latest": "npm run prepublish && npm publish --access public --tag latest",
    "publish:otp": "npm run prepublish && npm publish --access public --tag latest --otp",
    "publish:dry": "npm run prepublish && npm publish --dry-run"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@types/bun": "^1.3.8",
    "@types/config": "^3.3.5",
    "@types/i18n": "^0.13.12",
    "@types/i18next": "^13.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.2.1",
    "@types/sequelize": "^4.28.20",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "@vitest/coverage-istanbul": "^4.0.15",
    "eslint": "^9.39.1",
    "eslint-plugin-import": "^2.32.0",
    "knip": "^5.83.0",
    "mongodb-memory-server": "^11.0.1",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
